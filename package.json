{
	"name": "lx-starter-kit",
	"version": "1.0.1",
	"description": "Boilerplate: site with navigation",
	"main": "index.js",
	"scripts": {
		"analyze": "npm run clean && webpack --config build/webpack.config.js --mode production --analyze --progress",
		"build": "npm run clean && webpack --config build/webpack.config.js --mode production",
		"build:dev": "npm run clean && webpack --config build/webpack.config.js --mode development --progress",
		"clean": "rimraf ./dist",
		"coverage": "jest --config build/jest.config.js --coverage --watchAll=false",
		"dry": "npm run clean && node build/dry.js",
		"eslint": "eslint --cache --ext .ts,.tsx src",
		"eslint-dump": "eslint --print-config src/.eslintrc.json",
		"start": "webpack serve --config build/webpack.config.js --mode development --host 0.0.0.0",
		"test": "jest --config build/jest.config.js",
		"test-watch": "jest --config build/jest.config.js --watchAll"
	},
	"keywords": [
		"react",
		"typescript",
		"jest",
		"starter",
		"starter kit",
		"seed",
		"less",
		"webpack"
	],
	"author": "lexey111",
	"license": "ISC",
	"dependencies": {
		"mobx": "^6.1.8",
		"mobx-react": "^7.1.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.20",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.2",
		"@types/react-router": "^5.1.12",
		"@types/react-router-dom": "^5.1.7",
		"@types/resize-observer-browser": "^0.1.5",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.17.0",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
		"copy-webpack-plugin": "^8.0.0",
		"enzyme": "^3.11.0",
		"enzyme-to-json": "^3.6.1",
		"eslint": "^7.22.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-optimize-regex": "^1.2.0",
		"eslint-plugin-promise": "^4.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-sonarjs": "^0.6.0",
		"jest": "^26.6.3",
		"jest-enzyme": "^7.1.2",
		"less": "^4.1.1",
		"less-loader": "^8.0.0",
		"lorem-ipsum": "^2.0.3",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.5.3",
		"ts-loader": "^8.0.18",
		"typescript": "^4.2.3",
		"webpack": "^5.25.1",
		"webpack-bundle-analyzer": "^4.4.0",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^3.11.2"
	}
}
