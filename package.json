{
	"name": "lx-starter",
	"version": "1.0.0",
	"description": "Boilerplate: site with navigation",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && webpack --config build/webpack.config.js --mode production --progress",
		"analyze": "npm run clean && webpack --config build/webpack.config.js --mode production --analyze --progress",
		"dev": "npm run clean && webpack --config build/webpack.config.js --mode development --progress",
		"start": "webpack serve --config build/webpack.config.js --mode development",
		"clean": "rimraf ./dist",
		"eslint": "eslint --cache --ext .ts,.tsx src",
		"eslint-dump": "eslint --print-config src/.eslintrc.json",
		"test": "jest --config build/jest.config.js",
		"coverage": "jest --config build/jest.config.js --coverage --watchAll=false",
		"test-watch": "jest --config build/jest.config.js --watchAll"
	},
	"keywords": [
		"react",
		"typescript",
		"antd",
		"less",
		"webpack"
	],
	"author": "lexey111",
	"license": "ISC",
	"dependencies": {
		"mobx": "^6.0.2",
		"mobx-react": "^7.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.15",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"@types/resize-observer-browser": "^0.1.4",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.2.0",
		"copy-webpack-plugin": "^6.2.1",
		"enzyme": "^3.11.0",
		"enzyme-to-json": "^3.6.1",
		"eslint": "^7.12.1",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-optimize-regex": "^1.2.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-sonarjs": "^0.5.0",
		"jest": "^26.6.2",
		"jest-enzyme": "^7.1.2",
		"less": "^3.12.2",
		"less-loader": "^7.0.2",
		"lorem-ipsum": "^2.0.3",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.4.3",
		"ts-loader": "^8.0.7",
		"typescript": "^4.0.5",
		"webpack": "^5.3.2",
		"webpack-bundle-analyzer": "^3.9.0",
		"webpack-cli": "^4.1.0",
		"webpack-dev-server": "^3.11.0"
	}
}
